<header>
  <nav class="navbar navbar-expand-lg navbar-light bg-light">
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">

      <form class="form-inline mr-auto search-box">
        <input class="form-control" type="search" placeholder="Search" aria-label="Search">
        <button class="btn btn-success" type="submit"><i class="fa fa-search"></i></button>
      </form>


      <ul class="navbar-nav ml-auto">
        <li class="nav-item active">
          <a class="nav-link" href="#">Find A Provider</a>
        </li>

        <% if !user_signed_in? %>
          <li class="nav-item">
            <%= link_to 'Sign Up', new_user_registration_path, :class => 'nav-link' %>
          </li>
          <li class="nav-item">
            <%= link_to 'Sign In',  new_user_session_path, :class => 'nav-link'  %>
          </li>
        <% else %>
          <li class="nav-item">
            <%= link_to 'Account', edit_user_registration_path, :class => 'nav-link' %>
          </li>
          <li class="nav-item">
            <%= link_to 'Log Off',  destroy_user_session_path, :class => 'nav-link'  %>
          </li>
        <% end %>

        <li class="nav-item">
          <a class="nav-link" href="#">Contact</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="https://www.mefm.bc.ca/">ME & FM Society of BC</a>
        </li>
      </ul>

    </div>
  </nav>
</header>
